<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.huafeng_serve.modules.wallpaper.mapper.WallpaperMapper">
    <select id="selectByPage" resultType="com.example.huafeng_serve.modules.wallpaper.entity.Wallpaper">
        SELECT e.id,
               e.picUrl,
               e.dianji
        FROM pic AS e
        WHERE 1 = 1
    </select>
</mapper>